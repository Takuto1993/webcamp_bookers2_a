<div class='col-md-8 ofset-md-1'>
 <h2>Users</h2>
 
 <% @user.each do |user| %>
 
 <table class='table table-hover table-inverse'>
  <thead>
   <tr>
    <th>image</th>
    <th>name</th>
    <th></th>
    <th colspan="3"></th>
   </tr>
  </thead>
  <tbody>
   <tr>
    <td><%= image_tag user.get_profile_image(100,100) %></td>
    <td><%= user.name %></td>
    <td><%= link_to book.title, book_path(book.id) %></td>
   </tr>
  </tbody>
 </table>
 
 <% end %>
 
</div>
